                                </tr>
                            </thead>
                            <tbody>

                            {% for pprod in pprodList %}
                            <tr>
                                <td data-name="sel"><input type="checkbox" value="{{ pprod['parent_id'] }}"></td>
                                <td data-name="company" data-value="{{ pprod['company_id'] }}">{{ pprod['comp_name'] }}</td>
                                <td data-name="party">{{ pprod['party'] }}</td>
                                <td data-name="date-from">{{ pprod['date_from']|date('d.m.Y') }}</td>
                                <td data-name="date-to">{{ pprod['date_to']|date('d.m.Y') }}</td>
                                <td data-name="person">{{ pprod['person_name'] }}</td>
                                <td data-name="title">{{ pprod['title'] }}</td>
                                <td data-name="cnt">{{ pprod['cnt'] }}</td>
                                <td data-name="cnt_a">{{ pprod['cnt_a'] }}</td>
                                <td data-name="cnt_alost">{{ pprod['cnt_alost'] }}</td>
                                <td data-name="cnt_abad" >{{ pprod['cnt_abad'] }}</td>
                            </tr>
                            {% endfor %}

                            </tbody>
                            <tfoot>
                                <tr>
                                    <td colspan="11">
                                        <div class="pagination pagination-centered"></div>
                                    </td>
                                </tr>
