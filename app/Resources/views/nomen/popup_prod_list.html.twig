                        <table id="prod_list" class="table-striped footable-res footable metro-blue" data-page-size="12">
                            <thead>
                                <tr>
                                    <th ></th>
                                    <th data-title="company">Поставщик</th>
                                    <th data-title="party">Партия</th>
                                    <th data-title="date-from">Дата от</th>
                                    <th data-title="date-to">Дата до</th>
                                    <th data-title="person">Доступ</th>
                                    <th data-title="title">Наименование</th>
                                    <th data-title="cnt">На складе</th>
